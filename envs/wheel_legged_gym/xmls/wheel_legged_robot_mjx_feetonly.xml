  <contact>
    <exclude body1="base_link" body2="lf0_Link" />
    <exclude body1="lf0_Link" body2="lf1_Link" />
    <exclude body1="lf1_Link" body2="l_wheel_Link" />
    <exclude body1="base_link" body2="rf0_Link" />
    <exclude body1="rf0_Link" body2="rf1_Link" />
    <exclude body1="rf1_Link" body2="r_wheel_Link" />
  </contact>

  <sensor>
    <framepos name="imu_pos" objtype="site" objname="imu" />
    <framequat name="imu_quat" objtype="site" objname="imu" />
    <framelinvel name="imu_linvel" objtype="site" objname="imu" />
    <frameangvel name="imu_angvel" objtype="site" objname="imu" />
    <velocimeter name="imu_vel" site="imu" />
